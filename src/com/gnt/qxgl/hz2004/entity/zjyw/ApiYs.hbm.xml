<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
            name="com.gnt.qxgl.hz2004.entity.zjyw.ApiYs"
            table="ZJYW_API_YS"
            dynamic-insert="true"
            dynamic-update="true"
    >

        <id name="logid" column="LOGID">
            <generator class="uuid.hex"></generator>
        </id>
        <property name="lg" column="LG"/>
        <property name="nwbbz" column="NWBBZ"/>
        <property name="postid" column="POSTID"/>
        <property name="lkbm" column="LKBM"/>
        <property name="cjsj" type="java.util.Date" column="CJSJ"/>
    </class>

</hibernate-mapping>
