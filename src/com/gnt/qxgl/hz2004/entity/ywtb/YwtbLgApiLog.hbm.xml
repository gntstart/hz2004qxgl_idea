<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class
            name="com.gnt.qxgl.hz2004.entity.ywtb.YwtbLgApiLog"
            table="YWTB_API_LOG"
            dynamic-insert="true"
            dynamic-update="true"
    >

        <id name="logid" column="LOGID">
            <generator class="uuid.hex"></generator>
        </id>
        <property name="lgbm" column="LGBM"/>
        <property name="logsj" column="LOGSJ"/>
        <property name="apiname" column="APINAME"/>
        <property name="jls" column="JLS"/>
        <property name="bz" column="BZ"/>
        <property name="hs" column="HS"/>
        <property name="sfcg" column="SFCG"/>
        <property name="ip" column="IP"/>
    </class>

</hibernate-mapping>
